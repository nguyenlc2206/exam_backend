{
    "name": "exam_project",
    "version": "1.0.0",
    "description": "",
    "main": "src/main/server.ts",
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=production node dist/main/server.js",
        "build": "rimraf dist && tsc -p tsconfig.prod.json && tscpaths -p tsconfig.prod.json -s ./src -o ./dist",
        "postinstall": "tsc",
        "dev": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register --respawn --ignore-watch node_modules src/main/server.ts",
        "debug": "cross-env NODE_ENV=development tsnd --inspect=0.0.0.0:9222 --nolazy -r tsconfig-paths/register --respawn --ignore-watch node_modules src/main/server.ts",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/infrastructure/config/typeorm.config.ts",
        "typeorm:generate-migration": "npm run typeorm -- -d ./src/infrastructure/typeorm.config.ts migration:generate ./src/infrastructure/migrations/%npm_config_name%",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./src/infrastructure/migrations/%npm_config_name%",
        "typeorm:revert-migration": "npm run typeorm -- -d ./src/infrastructure/typeorm.config.ts migration:revert"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.10.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "pg": "^8.11.3",
        "rimraf": "^5.0.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.17",
        "uuid": "^9.0.1",
        "validator": "^13.11.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.15",
        "@babel/node": "^7.22.15",
        "@babel/preset-env": "^7.22.15",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.3",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/lodash": "^4.14.198",
        "@types/node": "^20.6.3",
        "@types/uuid": "^9.0.4",
        "@types/validator": "^13.11.1"
    }
}
